{
  "version": "2.1.0",
  "command": {
    "publish": {
      "ignoreChanges": [
        "*.md",
        "*.json",
        "*.txt",
        "test/**",
        "package.json"
      ]
    }
  },
  "npmClientArgs": [
    "--no-package-lock"
  ],
  "packages": [
    "packages/cli",
    "packages/components/*",
    "packages/configs",
    "packages/pos",
    "packages/store",
    "packages/styles",
    "packages/webpack"
  ]
}
