{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "svelte",
      "jest",
      "@testing-library/jest-dom"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}